# .env (template)
# - 이 파일은 로컬 개발용 기본값 템플릿
# - 실서비스/서버에는 값 채운 .env를 별도로 두는 걸 권장

# 런타임
APP_ENV=development
TZ=Asia/Seoul

# 프로젝트 식별자 (docker compose -p prefix, URL prefix에도 사용)
PROJECT_NAME=temp

# 외부 공개 포트 (호스트에서 접속하는 포트)
PORTAL_HTTP_PORT=18080

# 프론트 base path (포털에서 /{PROJECT_NAME}/ 로 접근하는 형태)
# 예: http://localhost:18080/temp/
VITE_BASE_PATH=/temp

# 프론트에서 API 호출 prefix
# - /api 로 두면: nginx가 /api/* 와 /{PROJECT_NAME}/api/* 둘 다 backend로 보내도록 설정 가능
VITE_API_PREFIX=/api

# 데이터 저장 경로(컨테이너 내부 기준)
DATA_ROOT=/data
DATA_WORK=/data/work
DATA_DB=/data/db
DATA_LOGS=/data/logs

# LLM/검색 연동 키 (비워두고 사용 시 채움)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4.1-mini
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=

# Vector DB (선택)
QDRANT_URL=
QDRANT_API_KEY=

# DB/캐시 (선택)
DB_HOST=
DB_PORT=5432
DB_NAME=
DB_USER=
DB_PASSWORD=
REDIS_HOST=
REDIS_PORT=6379
